--

local function GenTemp(FrameName, ButtonText, Gold, Holder)
	local Frame = Instance.new("Frame")
	Frame.Name = tostring(FrameName)
	Frame.Size = UDim2.new(1,0,0.075, 0)
	Frame.AnchorPoint = Vector2.new(0,0.5)
	Frame.BackgroundColor3 = Color3.fromRGB(255,255,255)
	local Corner = Instance.new("UICorner")
	Corner.CornerRadius = UDim.new(0.25,0)
	Corner.Parent = Frame
	local Gradient = Instance.new("UIGradient")
	Gradient.Rotation = 270
	if Gold == false then
		Gradient.Color = ColorSequence.new{
			ColorSequenceKeypoint.new(0,Color3.fromRGB(38,150,255)),
			ColorSequenceKeypoint.new(1,Color3.fromRGB(58,196,255))
		}
	elseif Gold == true then
		Gradient.Color = ColorSequence.new{
			ColorSequenceKeypoint.new(0,Color3.fromRGB(189, 191, 32)),
			ColorSequenceKeypoint.new(1,Color3.fromRGB(255, 252, 148))
		}
	end
	Gradient.Parent = Frame
	local Stroke = Instance.new("UIStroke")
	Stroke.Color = Color3.fromRGB(255,255,255)
	Stroke.Thickness = 3
	Stroke.Parent = Frame
	local Button = Instance.new("TextButton")
	Button.Size = UDim2.new(0.9,0,1,0)
	Button.AnchorPoint = Vector2.new(0.5,0)
	Button.Position = UDim2.new(0.5,0,0,0)
	Button.BackgroundTransparency = 1
	Button.Font = Enum.Font.GothamBlack
	Button.TextScaled = true
	Button.TextColor3 = Color3.fromRGB(255,255,255)
	Button.Text = tostring(ButtonText)
	Button.Parent = Frame
	Frame.Parent = Holder
end

GenTemp("1GoldAxe", "Axe Mod", true, Holder)
wait()
GenTemp("2Base", "Base", false, Holder)
wait()
GenTemp("3Sell", "Sell", false, Holder)
wait()
GenTemp("4BringWood", "Wood TP", false, Holder)
wait()
GenTemp("5Heaven", "Heaven TP", false, Holder)
wait()
GenTemp("6Space", "Space TP", false, Holder)
wait()
GenTemp("7Custom", "Custom", false, Holder)
wait()
GenTemp("1Speed", "Speed", false, Holder2)
wait()
GenTemp("2MaxCam", "Max Cam", false, Holder2)
wait()
GenTemp("3ShiftDel", "Shift Delete", false, Holder2)
wait()
GenTemp("4CtrlTP", "Ctrl TP", false, Holder2)
wait()
GenTemp("5SellWood", "Sell Wood", false, Holder2)
wait()
GenTemp("6BaseWood", "Wood 2 Base", false, Holder2)

